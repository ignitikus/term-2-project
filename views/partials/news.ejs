<div class="tab-pane fade show active" id="news" role="tabpanel" aria-labelledby="news-tab">
   <form action="/" method="GET">
      <div class="input-group mb-3">
         <input type="text" class="form-control" placeholder='Search' aria-label="Default" aria-describedby="inputGroup-sizing-default" style="margin-top: 17px;" name='searchForNews'>
      </div>
   </form>
   <% data.articles.forEach(({title, url, urlToImage, description}) =>{ %>
      <div class="container">
         <div class="jumbotron">
            <div style="display: flex;">
               <div>
               <h1><%= title %> </h1>
               <p style="font-size: large;"><%= description %> </p>
               </div>
               <img src="<%= urlToImage%>" alt="<%= title%>" class="article-image">
            </div>
            <a href="<%= url%>" target="_blank">Source</a>
         </div>
      </div>
   <%}) %> 
</div>